{
  "name": "@flybase/react-ontology-ribbon",
  "version": "0.2.1",
  "description": "A react component for generating heatmap type displays for ontology slims.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files",
    "clean": "nwb clean-module && npm clean-demo",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "format": "prettier --write 'src/**/*.js'"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "nwb": "^0.23.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2"
  },
  "author": "Jim Thurmond",
  "contributors": [
    {
      "name": "Josh Goodman"
    }
  ],
  "homepage": ".",
  "license": "MIT",
  "repository": "https://github.com/FlyBase/react-ontology-ribbon",
  "keywords": [
    "react-component",
    "Gene Ontology",
    "slim"
  ],
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "tabWidth": 2,
    "printWidth": 80
  },
  "publishConfig": {
    "access": "public"
  }
}
